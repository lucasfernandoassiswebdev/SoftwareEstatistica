@{ Layout = null; }

<div class="col s10 offset-s1">
    <h5 class="left">Tabela</h5>
    <table class="tabela highlight" id="dadosOrdenados" style="margin-bottom: 30px">
        <thead>
            <tr>
                <td>VaP</td>
                <td>Fr</td>
                <td>FrP</td>
                <td>FrA</td>
                <td>FrAP</td>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>   

    <script>
        calculaFrequencias(String('@ViewBag.Dados'.replace(/&quot;/g,'"'))).forEach(function(item){
            $("#dadosOrdenados tbody").append($("<tr>",{
                html: [ $("<td>",{
                            html: [item.Var]
                        }),
                        $("<td>",{
                            html: [item.Fr]
                        }),
                        $("<td>",{
                            html: [item.FrP]  
                        }),
                        $("<td>",{
                            html: [item.FrA]
                        }),
                        $("<td>",{
                            html: [item.FrAP]
                        }) ]
            }));
        });
    </script>  